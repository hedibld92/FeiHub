{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "VUE_APP_SUPABASE_URL": "@vue_app_supabase_url",
    "VUE_APP_SUPABASE_ANON_KEY": "@vue_app_supabase_anon_key"
  }
} 